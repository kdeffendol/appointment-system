<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CustomerTableViewScreenController">
   <children>
      <TableView fx:id="customerTableView" layoutX="11.0" layoutY="83.0" prefHeight="434.0" prefWidth="578.0">
        <columns>
          <TableColumn fx:id="customerIdColumn" prefWidth="86.0" text="CustomerID" />
          <TableColumn fx:id="nameTableColumn" prefWidth="87.0" text="Name" />
            <TableColumn fx:id="addressTableColumn" prefWidth="65.0" text="Address" />
            <TableColumn fx:id="firstDivisionTableColumn" prefWidth="102.0" text="State/Province" />
            <TableColumn fx:id="countryTableColumn" prefWidth="65.0" text="Country" />
            <TableColumn fx:id="postalCodeTableColumn" prefWidth="81.0" text="Postal Code" />
            <TableColumn fx:id="phoneNumberTableColumn" prefWidth="91.0" text="Phone Number" />
        </columns>
      </TableView>
      <HBox alignment="CENTER_LEFT" layoutX="19.0" layoutY="512.0" prefHeight="74.0" prefWidth="562.0" spacing="20.0">
         <children>
            <Button fx:id="addCustomerButton" mnemonicParsing="false" onAction="#addCustomerButtonPushed" prefHeight="50.0" prefWidth="130.0" text="Add ">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="updateCustomerButton" mnemonicParsing="false" onAction="#updateCustomerButtonPushed" prefHeight="50.0" prefWidth="130.0" text="Update">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="deleteCustomerButton" mnemonicParsing="false" onAction="#deleteCustomerButtonPushed" prefHeight="50.0" prefWidth="130.0" text="Delete">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonPushed" prefHeight="50.0" prefWidth="130.0" text="Back">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <Label layoutX="11.0" layoutY="14.0" prefHeight="50.0" prefWidth="310.0" text="Customer Table">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
