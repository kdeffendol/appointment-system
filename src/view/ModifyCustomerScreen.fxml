<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ModifyCustomerScreenController">
   <children>
      <Label layoutX="24.0" layoutY="23.0" prefHeight="56.0" prefWidth="150.0" text="Modify Customer">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <VBox layoutX="99.0" layoutY="79.0" prefHeight="440.0" prefWidth="408.0">
         <children>
            <HBox prefHeight="50.0" prefWidth="408.0">
               <children>
                  <Label prefHeight="47.0" prefWidth="150.0" text="Customer Id">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="customerIdTextField" disable="true" prefHeight="45.0" prefWidth="248.0">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="408.0">
               <children>
                  <Label prefHeight="81.0" prefWidth="150.0" text="Name">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="nameTextField" prefHeight="45.0" prefWidth="248.0">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="408.0">
               <children>
                  <Label prefHeight="75.0" prefWidth="150.0" text="Address">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="addressTextField" prefHeight="45.0" prefWidth="248.0">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="408.0">
               <children>
                  <Label prefHeight="50.0" prefWidth="150.0" text="Country">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="countryComboBox" onAction="#firstDivisionComboBoxClicked" prefHeight="40.0" prefWidth="170.0" promptText="Country" />
               </children>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="408.0">
               <children>
                  <Label prefHeight="50.0" prefWidth="150.0" text="State/Province">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="firstDivisionComboBox" prefHeight="40.0" prefWidth="170.0" promptText="State/Province" />
               </children>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="408.0">
               <children>
                  <Label prefHeight="75.0" prefWidth="150.0" text="Postal Code">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="postalCodeTextField" prefHeight="45.0" prefWidth="248.0">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="408.0">
               <children>
                  <Label prefHeight="75.0" prefWidth="150.0" text="Phone Number">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
                  <TextField fx:id="phoneNumberTextField" prefHeight="45.0" prefWidth="248.0">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </TextField>
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox alignment="BASELINE_RIGHT" layoutX="206.0" layoutY="530.0" prefHeight="66.0" prefWidth="318.0" spacing="30.0">
         <children>
            <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveButtonPushed" prefHeight="35.0" prefWidth="100.0" text="Save" />
            <Button fx:id="cancelButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#cancelButtonPushed" prefHeight="35.0" prefWidth="100.0" text="Cancel" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
